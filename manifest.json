{
    "manifest_version": 3,
    "name": "DBoost",
    "version": "1.0.0",
    "description": "Расширение для ускорения работы Discord на случай блокировки.",
    "permissions": ["proxy", "storage", "alarms"],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "pages/popup.html"
    },
    "background": {
        "service_worker": "service/background.js"
    }
}
